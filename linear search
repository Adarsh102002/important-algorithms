#include<iostream>
using namespace std;
int main()
{
    cout<<"enter the size of array";
    int n;
    int res=0;
    cin>>n;
    int a[n];
    cout<<"enter the elements of array";
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"enter the element to be searched";
    int t;
    cin>>t;
    for(int i=0;i<n;i++)
    {
       
            if(a[i]==t)
            {
                cout<<"element found";
                 res=1;
                break;
            }
    }
        if(res==0)
        cout<<"element not found";
        return 0;
  }
